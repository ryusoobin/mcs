<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mcs">
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Mcs">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainLogin" />
        <activity android:name=".MainRegister" />
        <activity android:name=".MainPay" />
        <activity android:name=".MainEct" />
        <activity android:name=".MainReservation" />
        <activity android:name=".MainMypage" />
        <activity android:name=".InformationFindActivity" />
        <activity android:name=".PayCompleteActivity" />
        <activity android:name=".ReserveCompleteActivity" />
        <activity android:name=".UsageHistoryActivity" />
    </application>

</manifest>